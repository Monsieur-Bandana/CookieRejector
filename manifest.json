{
    "manifest_version": 2,
    "name": "Cookie-Rejector",
    "version": "1.0",
    "description": "Autoselect reject non mandatory cookies",
    "icons": {
        "48": "icons/reject-cookies.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.faz.net/*"
            ],
            "js": [
                "rejectcookies.js"
            ]
        }
    ]
}